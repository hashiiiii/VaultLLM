# Terraform providers via Git LFS
terraform/.terraform/providers/**/* filter=lfs diff=lfs merge=lfs -text 
*.mdc filter=lfs diff=lfs merge=lfs -text
*.backup filter=lfs diff=lfs merge=lfs -text
*.yml filter=lfs diff=lfs merge=lfs -text
*.gitignore filter=lfs diff=lfs merge=lfs -text
*.hcl filter=lfs diff=lfs merge=lfs -text
*.txt filter=lfs diff=lfs merge=lfs -text
*.0_x5 filter=lfs diff=lfs merge=lfs -text
*.tf filter=lfs diff=lfs merge=lfs -text
*.tfstate filter=lfs diff=lfs merge=lfs -text
terraform/.terraform/providers/**/* filter=lfs diff=lfs merge=lfs -text
*.tf !text !filter !merge !diff
*.yml !text !filter !merge !diff
.cursor/rules/* !text !filter !merge !diff
terraform/.terraform.lock.hcl !text !filter !merge !diff
